<view class="container"><view class="header"><image class="bg-title" src="/static/images/bbb.png"></image><view class="header-title-common"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e" class="_p"></view><view class="_b">选择刷卡通道</view></view></view><view class="_h1"><view class="_p">信用卡刷卡</view><view class="_p">{{amount+"元"}}</view></view><view class="_h2">选择刷卡通道</view><view class="_ul"><block wx:for="{{passageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['choose',['$0','$1','$2'],[[['passageList','',index,'id']],[['passageList','',index,'needCity']],[['passageList','',index,'needCode']]]]]]]}}" bindtap="__e" class="_li"><view class="_h3"><image src="/static/images/yl-icon@2x.png" mode="widthFix"></image><view class="_b">{{item.alias}}</view></view><view class="_h4"><view class="_p">{{"单笔限额："+item.limitMin+"-"+item.limitMax+"元"}}</view><view class="_p">{{"单日限额："+item.dayMax+"元"}}</view><view class="_p">{{"交易时间："+item.startTime+"-"+item.endTime}}</view><view class="_p">{{"交易费率："+item.costRate+"+"+item.costFee}}</view></view><view data-event-opts="{{[['tap',[['limits',['$0','$1'],[[['passageList','',index,'id']],[['passageList','',index,'alias']]]]]]]}}" catchtap="__e" class="_h5">查看限额</view></view></block></view></view>