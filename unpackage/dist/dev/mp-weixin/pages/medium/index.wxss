.bg-title {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.header {
  width: 100%;
  position: relative;
  padding: 30rpx;
  padding-top: calc(25px + 30rpx);
  height: 207rpx;
  background-color: #46c7a9;
}
.header .header-title-common {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
  position: relative;
}
.header .header-title-common ._b {
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: white;
}
.header .header-title-common ._p:first-child {
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
  border-bottom: 5rpx solid white;
  border-left: 5rpx solid white;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 0;
}
.header .header-title-common ._p:last-child {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #F7CF9E;
  position: absolute;
  right: 0;
}
.tips-part {
  width: 690rpx;
  height: 69rpx;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 22rpx 2rpx rgba(67, 67, 67, 0.13);
  border-radius: 10rpx;
  margin: 30rpx auto;
  display: flex;
  align-items: center;
}
.tips-part image {
  width: 35rpx;
  height: 35rpx;
  margin-left: 20rpx;
}
.tips-part input {
  width: 500rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
}
.tips-part view {
  margin-left: 20rpx;
  color: #46c7a9;
}
.users {
  width: 750rpx;
  height: 65vh;
  margin-top: 50rpx;
  display: flex;
}
.users .jiazai {
  color: #999999;
  display: flex;
  justify-content: center;
}
.users .vv {
  box-shadow: 0px 8rpx 22rpx 2rpx rgba(67, 67, 67, 0.13);
  display: flex;
  height: 135rpx;
  width: 691rpx;
  margin-left: 30rpx;
  align-items: center;
}
.users .vv image {
  width: 62rpx;
  height: 62rpx;
  margin-left: 29rpx;
}
.users .vv .vv2 {
  font-size: 24rpx;
  margin-left: 30rpx;
}
.users .vv .vv2 view {
  margin-top: 10rpx;
}
.users .vv .vv2 .name {
  font-size: 30rpx;
}
.users .vv .vv2 .name1 {
  color: white;
  background-color: #46c7a9;
  height: 34rpx;
  width: 128rpx;
  display: flex;
  justify-content: center;
  border-radius: 10rpx;
  align-items: center;
}
.users .vv .shanchu {
  width: 32rpx;
  height: 32rpx;
  margin-left: 50rpx;
}
.btn {
  height: 86rpx;
  background-color: #46c7a9;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: white;
  position: fixed;
  bottom: 0px;
  width: 100%;
  left: 0;
}
.btn image {
  margin-right: 20rpx;
  width: 30rpx;
  height: 30rpx;
}

