<view class="container"><view class="header"><view class="header-title-common"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e" class="_p"></view><view class="_b">{{name}}</view></view></view><block wx:if="{{myteams.length==0}}"><view class="empty"><view><image src="{{empty}}" mode="widthFix"></image></view><view class="tits">{{message}}</view></view></block><view class="main-box"><view class="search-content"><view class="search-box"><image src="/static/images/kef/sec@2x.png" mode="widthFix"></image><input type="text" placeholder="请输入手机号或者姓名" data-event-opts="{{[['input',[['__set_model',['','fullname','$event',[]]]]]]}}" value="{{fullname}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getDirectList',[1,0]]]]]}}" bindtap="__e" class="_p">搜索</view></view></view><view class="_ul"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="_li"><view class="user-box"><image src="/static/head.jpg"></image><view class="right-box"><view class="_h1"><view class="_p"><block wx:if="{{item.$orig.fullname==undefined}}"><block>未实名</block></block><block wx:else><block>{{item.g0+'*'+item.g1}}</block></block><block wx:if="{{item.$orig.vipLevel>0&&item.$orig.vipLevel<=3}}"><text>{{"钻石"+item.$orig.vipLevel}}</text></block><block wx:else><block wx:if="{{item.$orig.vipLevel>3}}"><text>钻石3</text></block><block wx:else><text>普通</text></block></block></view><view class="_p">{{''+item.g2+"****"+item.g3+''}}<image src="/static/images/kef/phone.png" mode="widthFix"></image></view></view><view class="_h1"><navigator class="_a"></navigator><navigator class="_a">{{"注册时间:"+item.$orig.createTime}}</navigator></view></view></view><view class="data-box"><view class="_p">直推人数：<text>{{item.$orig.firstCount}}</text></view><view class="_p">团队人数:<text>{{item.$orig.teamCount}}</text></view><view class="_p">直推激活：<text>{{item.$orig.firstActivationCount}}</text></view><view class="_p">本月交易量(元)：<text>{{item.$orig.curMonthAllAmount}}</text></view></view></view></block></view></view></view>