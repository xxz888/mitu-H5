.bg-title {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.header {
  width: 100%;
  position: relative;
  padding: 30rpx;
  padding-top: calc(25px + 30rpx);
  height: 207rpx;
}
.header .header-title-common {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
  position: relative;
}
.header .header-title-common ._b {
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #F7CF9E;
}
.header .header-title-common ._p:first-child {
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
  border-bottom: 5rpx solid #D4C09D;
  border-left: 5rpx solid #D4C09D;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 0;
}
.header .header-title-common ._p:last-child {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #F7CF9E;
  position: absolute;
  right: 0;
}
.main-box {
  padding: 30rpx;
}
.main-box .card-part {
  box-shadow: 0px 8rpx 22rpx 2rpx rgba(67, 67, 67, 0.13);
  border-radius: 20rpx;
}
.main-box .card-detail {
  padding: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.main-box .card-detail ._p {
  width: 50%;
  margin-top: 20rpx;
  font-size: 27rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.main-box .card-box {
  display: flex;
  position: relative;
  height: 159rpx;
  align-items: center;
  padding: 20rpx 40rpx;
}
.main-box .card-box .card-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.main-box .card-box .logo {
  width: 60rpx;
  height: 60rpx;
  margin-right: 33rpx;
}
.main-box .card-box ._h1 {
  flex: 1;
}
.main-box .card-box ._h1 ._b {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #F7CF9E;
  margin-bottom: 20rpx;
}
.main-box .card-box ._h1 ._b ._a {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #F7CF9E;
}
.main-box .card-box ._h1 ._p {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #F7CF9E;
}
.btn {
  height: 86rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  margin: 0 auto;
  background: linear-gradient(269deg, #D79F65 0%, #FDD8AA 100%);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
._ul {
  box-shadow: 0px 8rpx 22rpx 2rpx rgba(67, 67, 67, 0.13);
  border-radius: 20rpx;
  margin-top: 20rpx;
  padding: 30rpx 0;
  height: 600rpx;
  overflow-y: scroll;
}
._ul ._li:first-child {
  margin-bottom: 40rpx;
}
._ul ._li {
  margin-bottom: 20rpx;
  display: flex;
}
._ul ._li ._b {
  font-size: 33rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  text-align: center;
}
._ul ._li ._b:nth-child(1) {
  flex: 4;
}
._ul ._li ._b:nth-child(2) {
  flex: 1;
}
._ul ._li ._b:nth-child(3) {
  flex: 2;
}
._ul ._li ._b:nth-child(4) {
  flex: 2;
}
._ul ._li ._p {
  font-size: 24rpx;
  font-family: PingFang SC;
  color: #888;
  text-align: center;
}
._ul ._li ._p:nth-child(1) {
  flex: 4;
}
._ul ._li ._p:nth-child(2) {
  flex: 1;
}
._ul ._li ._p:nth-child(3) {
  flex: 2;
}
._ul ._li ._p:nth-child(4) {
  flex: 2;
  color: #F9CB32;
}
.container .modalimage {
  width: 50rpx;
  height: 50rpx;
}
.container .modal-box {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .modal-content {
  background-color: #fff;
  width: 600rpx;
  height: 600rpx;
  border-radius: 10rpx;
  position: relative;
}
.container .modal-content .bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.container .modal-content ._h1 {
  position: relative;
  z-index: 1;
  font-size: 43rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #FDFAF9;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-shadow: 0px 3px 3px rgba(46, 46, 46, 0.48);
}
.container .modal-content ._h4 {
  position: relative;
  z-index: 1;
  font-size: 43rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #FDFAF9;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-shadow: 0px 3px 3px rgba(46, 46, 46, 0.48);
}
.container .modal-content .confirm-box {
  position: relative;
  z-index: 1;
  display: flex;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 22rpx 2px rgba(67, 67, 67, 0.2);
  border-radius: 20rpx;
  width: 500rpx;
  height: 350rpx;
  margin: 10rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .modal-content .confirm-box ._a {
  width: 100%;
  height: 350rpx;
  font-size: 20rpx;
}
.container .modal-content .confirm-box ._a ._p {
