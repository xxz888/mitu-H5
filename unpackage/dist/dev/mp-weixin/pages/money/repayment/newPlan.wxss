}
.container .modal-content .confirm-box ._h2 {
  color: #666666;
  margin: 120rpx auto 90rpx;
  font-size: 36rpx;
  font-weight: normal;
}
.container .modal-content .confirm-box view {
  display: flex;
}
.container .modal-content .confirm-box view view {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #f7f7f7;
  color: #F0AD4E;
  width: 140rpx;
  height: 70rpx;
  margin: 120rpx auto 90rpx;
  font-size: 18rpx;
}
.container .modal-content .confirm-box view input {
  color: #666666;
  margin: 120rpx auto 90rpx;
  font-size: 30rpx;
  font-weight: normal;
  width: 250rpx;
  padding: 0 20rpx;
  height: 70rpx;
  box-sizing: border-box;
  background-color: #f7f7f7;
}
.container .modal-content .confirm-box ._h3 {
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .modal-content .confirm-box ._h3 ._p {
  font-size: 27rpx;
  color: #333;
  background: #F5F3F3;
  width: 200rpx;
  border-radius: 5rpx;
  text-align: center;
  line-height: 60rpx;
}
.container .modal-content .confirm-box ._h3 ._p:last-child {
  background-color: #FAD6A8;
  margin-left: 50rpx;
}

