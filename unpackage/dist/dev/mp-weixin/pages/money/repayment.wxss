.bg-title {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.header {
  width: 100%;
  position: relative;
  padding: 30rpx;
  padding-top: calc(25px + 30rpx);
  height: 207rpx;
  background-color: #46c7a9;
}
.header .header-title-common {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
  position: relative;
}
.header .header-title-common ._b {
  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: white;
}
.header .header-title-common ._p:first-child {
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
  border-bottom: 5rpx solid white;
  border-left: 5rpx solid white;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  left: 0;
}
.header .header-title-common ._p:last-child {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #F7CF9E;
  position: absolute;
  right: 0;
}
.tips-part {
  width: 690rpx;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 22rpx 2rpx rgba(67, 67, 67, 0.13);
  border-radius: 20rpx;
  margin: 30rpx auto;
  padding: 30rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.tips-part ._p {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}
.tips-part ._p::before {
  content: '';
  width: 10rpx;
  height: 10rpx;
  display: inline-block;
  margin-right: 10rpx;
  border-radius: 50%;
  background-color: #F4CA99;
}
._li {
  box-sizing: border-box;
  width: 690rpx;
  height: 285rpx;
  position: relative;
  margin: 0 auto;
  margin-top: 20rpx;
  padding: 0 40rpx;
}
._li .bg {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
  left: 0;
  top: 0;
}
._li .card-btm-part {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: white;
  height: 85rpx;
}
._li .card-top-part {
  position: relative;
  height: 190rpx;
  width: 100%;
  z-index: 4;
  color: white;
  border-bottom: 1rpx solid white;
}
._li .card-top-part .del {
  position: absolute;
  left: -20rpx;
  width: 30rpx;
  top: 30rpx;
}
._li .card-top-part .logo {
  width: 40rpx;
  position: absolute;
  top: 20rpx;
  right: 0;
}
._li .card-top-part ._h1 {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  position: absolute;
  left: 156rpx;
  top: 50rpx;
}
._li .card-top-part ._h2 {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: white;
  position: absolute;
  left: 156rpx;
  top: 120rpx;
  display: flex;
  align-items: center;
}
._li .card-top-part ._h2 text {
  display: block;
  font-size: 22rpx;
  padding-left: 30rpx;
}
._li .card-top-part ._h3 {
  position: absolute;
  top: 50rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: white;
  right: -10rpx;
  border: 1rpx solid white;
  padding: 3rpx 10rpx;
  font-size: 20rpx;
  border-radius: 6rpx;
}
._li .card-top-part .logo-icon {
  width: 55rpx;
  height: 55rpx;
  position: absolute;
  top: 50rpx;
  left: 50rpx;
}
.btn {
  height: 86rpx;
  background-color: #46c7a9;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: white;
  position: fixed;
  bottom: 0px;
  width: 100%;
  left: 0;
}
.btn image {
  margin-right: 20rpx;
  width: 30rpx;
  height: 30rpx;
}

