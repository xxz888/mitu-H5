.bg-title {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.modal-box {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
}
.modal-content {
  width: 600rpx;
  height: 600rpx;
  border-radius: 10rpx;
  position: relative;
}
.modal-content .bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.modal-content ._h1 {
  position: relative;
  z-index: 1;
  font-size: 43rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #FDFAF9;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-shadow: 0px 3px 3px rgba(46, 46, 46, 0.48);
}
.modal-content ._h4 {
  position: relative;
  z-index: 1;
  font-size: 43rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #FDFAF9;
  height: 100rpx;
  display: flex;
  margin: 55px 0 10px;
  align-items: center;
  justify-content: center;
  text-shadow: 0px 3px 3px rgba(46, 46, 46, 0.48);
}
.modal-content .confirm-box {
  position: relative;
  z-index: 1;
  display: flex;
  width: 500rpx;
  height: 350rpx;
  margin: 10rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.modal-content .confirm-box ._a {
  width: 80%;
  height: 280rpx;
  font-size: 36rpx;
  padding-top: 20rpx;
}
.modal-content .confirm-box ._a ._p {
  display: flex;
  justify-content: center;
  align-items: center;
}
.modal-content .confirm-box ._a ._p view {
  display: flex;
  width: 50%;
}
.modal-content .confirm-box image {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  top: -30rpx;
}
.modal-content .confirm-box ._h2 {
  color: #666666;
  margin: 120rpx auto 90rpx;
  font-size: 36rpx;
  font-weight: normal;
}
.modal-content .confirm-box view {
  display: flex;
}
.modal-content .confirm-box view view {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #f7f7f7;
  color: #F0AD4E;
  width: 140rpx;
  height: 70rpx;
  margin: 120rpx auto 90rpx;
  font-size: 18rpx;
}
.modal-content .confirm-box view input {
  color: #666666;
  margin: 120rpx auto 90rpx;
  font-size: 30rpx;
  font-weight: normal;
  width: 250rpx;
  padding: 0 20rpx;
  height: 70rpx;
  box-sizing: border-box;
  background-color: #f7f7f7;
}
.modal-content .confirm-box ._h3 {
  display: flex;
  align-items: center;
  justify-content: center;
}
.modal-content .confirm-box ._h3 ._p {
  font-size: 27rpx;
  color: #333;
  background: #F5F3F3;
  width: 200rpx;
  border-radius: 5rpx;
  text-align: center;
  line-height: 60rpx;
}
.modal-content .confirm-box ._h3 ._p:last-child {
  background-color: #46c7a9;
  margin-left: 50rpx;
}
.header {
  width: 100%;
  height: 130rpx;
  background-color: #46c7a9;
  text-align: center;
  color: white;
  font-size: 40rpx;
  padding-top: 60rpx;
  margin-bottom: 30rpx;
}
.carousel {
  width: 100%;
  height: 230rpx;
}
.carousel .carousel-item {
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding: 20rpx;
}
.carousel image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.notice-box {
  display: flex;
  width: 690rpx;
  height: 71rpx;
  align-items: center;
  background: #FFFFFF;
  box-shadow: 0px 4rpx 9rpx 1rpx rgba(58, 58, 58, 0.05);
  border-radius: 10rpx;
  margin: -30rpx auto 0;
  padding: 0 27rpx;
}
.notice-box image {
  width: 40rpx;
}
.notice-box image:first-child {
  width: 30rpx;
  margin-right: 10rpx;
}
.notice-box ._p {
  font-size: 27rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  margin-left: 50rpx;
  width: 400rpx;
}
.notice-box ._b {
  font-size: 27rpx;
  font-family: PingFang SC;
  font-weight: normal;
  color: #999;
  margin-left: auto;
}
.notice-box .text-overflow-1 {
  color: #999;
  font-size: 25rpx;
}
.center {
  width: 680rpx;
  margin: 20rpx auto;
}
.center .xk {
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-bottom: 20rpx;
}
.center .xk .up {
  width: 334rpx;
  height: 160rpx;
  position: relative;
}
.center .xk .up image {
  width: 100%;
  height: 160rpx;
  position: absolute;
  z-index: -1;
}
.center .xk .up .text {
  color: white;
  padding: 30rpx 22rpx;
}
.center .xk .up .text ._p {
  font-size: 28rpx;
}
.center .xk .up .text ._b {
  font-size: 18rpx;
}
.nav {
  position: relative;
  padding: 40rpx 0;
  width: 680rpx;
  margin: 0 auto;
}
.nav .nav_bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 300rpx;
  z-index: -1;
}
._ul {
  margin: 0 auto;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  justify-content: space-around;
}
._ul ._li {
  display: flex;
  align-items: center;
  flex-direction: column;
  width: 160rpx;
  margin-bottom: 40rpx;
}
._ul ._li ._h2 {
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
}
._ul ._li image {
  width: 64rpx;
}
.nav_color {
  color: white;
}
._h1 {
  display: flex;
  align-items: center;
  padding: 20rpx 40rpx;
}
._h1 ._p {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  margin-left: 10rpx;
}
._h1 image {
  width: 6rpx;
  height: 30rpx;
}
.fn-box {
  padding: 0 30rpx 100rpx 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.fn-box image {
  width: 98%;
}
.fn-box view {
  width: 100%;
  display: flex;
  align-items: center;
}
.fn-box view image {
  flex: 1;
}

